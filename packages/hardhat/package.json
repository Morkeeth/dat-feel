    "compile": "hardhat compile && yarn postdeploy",
    "deploy": "hardhat deploy --export-all ../frontend/src/generated/contracts/hardhat_contracts.json",
    "postdeploy": "hardhat run scripts/publish.ts",
    "watch": "ts-node scripts/watch.js",
    "accounts": "hardhat accounts",
